{
  "name": "top-sites",
  "description": "List of the top 500 domains and pages on the web",
  "homepage": "https://documentup.com/Kikobeats/top-sites",
  "version": "1.0.2",
  "main": "index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kikobeats/top-sites.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/top-sites/issues"
  },
  "keywords": [
    "best",
    "domains",
    "moz",
    "pages",
    "rank",
    "sites",
    "top",
    "top500",
    "websites"
  ],
  "dependencies": {
    "get-stdin": "~6.0.0",
    "json-future": "~2.1.2",
    "papaparse": "~4.6.0"
  },
  "devDependencies": {
    "ava": "latest",
    "coveralls": "latest",
    "finepack": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "nyc": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "index.js",
    "scripts"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard-markdown README.md && standard",
    "postinstall": "curl -sL https://moz.com/top500/domains/csv | ./scripts/postinstall",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js {core,test,bin}/**/*.js --single-quote",
    "test": "nyc ava test --verbose"
  },
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ]
  }
}
